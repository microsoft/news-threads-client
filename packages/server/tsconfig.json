{
	"extends": "../../tsconfig",
	"compilerOptions": {
		"target": "ES2017",
		"module": "CommonJS",
		"outDir": "build",
		"allowJs": true,
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
		"resolveJsonModule": true,
		"sourceMap": true,
		"noImplicitAny": false
	},
	"include": ["src/**/*"]
}
